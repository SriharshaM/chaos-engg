<!-- MAIN CONTENT HERE-->
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">AWS Chaos</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/">Home</a></li>
              <li class="breadcrumb-item active">AWS</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Main row -->
        <div class="stepper-wrapper">
            <div class="stepper-item completed">
              <div class="step-counter"><i class="fas fa-check"></i></div>
              <div class="step-name">Credentials</div>
            </div>
            <div class="stepper-item completed">
              <div class="step-counter"><i class="fas fa-check"></i></div>
              <div class="step-name">Service</div>
            </div>
            <div class="stepper-item completed">
              <div class="step-counter"><i class="fas fa-check"></i></div>
              <div class="step-name">Activity</div>
            </div>
            <div class="stepper-item completed">
              <div class="step-counter"><i class="fas fa-check"></i></div>
              <div class="step-name">Parameters</div>
            </div>
            <div class="stepper-item active">
                <div class="step-counter">5</div>
                <div class="step-name">Result</div>
            </div>
          </div>
        </div><!-- /.container-fluid -->
        <div class="row">
            <div class="col-md-8">
                <div class="card card-primary">
                    <div class="card-header">
                        <h3 class="card-title">Experiment results</h3>
                    </div>
                    <div class="card-body">
                        <p>
                            <h4>Status update</h4><br>
                            <% if(json_type === 'start'){ %>
                                <% if(error_encountered){ %>
                                    <h3 style="color: red">Error encountered</h3>
                                <% } else { %>
                                    <% var resp_complete = "" %>
                                    <% for(i in json_response) { %>
                                        Instance ID : <%= json_response[i].InstanceId %><br>
                                        Current state : <%= json_response[i].CurrentState.Name %><br>
                                        Previous state : <%= json_response[i].PreviousState.Name %><br><br>
                                        <% resp_complete += JSON.stringify(json_response[i], null, 4) + ',' %>
                                    <% } %>
                                <% } %>
                                Response complete :<br> <pre><%= resp_complete %></pre><br>
                            <% } else if(json_type === 'stop') { %>  
                                <% for(i in json_response.StoppingInstances) { %>
                                    <% if(error_encountered){ %>
                                        <h3 style="color: red">Error encountered</h3>
                                    <% } else { %>
                                        Instance ID : <%= json_response.StoppingInstances[i].InstanceId %><br>
                                        Current state : <%= json_response.StoppingInstances[i].CurrentState.Name %><br>
                                        Previous state : <%= json_response.StoppingInstances[i].PreviousState.Name %><br><br>
                                    <% } %>
                                <% } %>
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
				<%= console.log('HTTPStatusCode') %>
				<%= console.log(json_response.ResponseMetadata.HTTPStatusCode) %>
                            <% } else if(json_type === 'restart') { %>  
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
			    <% } else if(json_type === 'describedb') { %>  
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
			    <% } else if(json_type === 'failover') { %>  
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
			    <% } else if(json_type === 'startexperiment') { %>
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
			    <% } else if(json_type === 'stopexperiment') { %>
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
			    <% } else if(json_type === 'getexperiment') { %>
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
                            <% } else if(json_type === 'terminate') { %> 
                                <% for(i in json_response.TerminatingInstances) { %>
                                    <% if(error_encountered){ %>
                                        <h3 style="color: red">Error encountered</h3>
                                    <% } else { %>
                                        Instance ID : <%= json_response.TerminatingInstances[i].InstanceId %><br>
                                        Current state : <%= json_response.TerminatingInstances[i].CurrentState.Name %><br>
                                        Previous state : <%= json_response.TerminatingInstances[i].PreviousState.Name %><br><br>
                                    <% } %> 
                                <% } %> 
                                Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
                            <% } else { %>  
                                <% if(error_encountered){ %>
                                    <h3 style="color: red">Error encountered</h3>
                                    Response complete :<br> <pre><%= json_response %><br>
                                <% } else { %>
                                    Response complete :<br> <pre><%= JSON.stringify(json_response, null, 4) %></pre><br>
                                <% } %>
                            <% } %>
                        </p>
                    </div>
                    <div class="card-footer">
                        <a href="/aws/ec2" class="btn btn-default">Try another experiment</a>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Action History</h3>
                    </div>
                    <div class="card-body">
                        <ul class="products-list product-list-in-card pr-2">
                            <% if(user_history.length > 0) { %>
                                <% for(i in user_history) { %>
                                    <li class="item">
                                        <div class="product-title">
                                            <%= user_history[i].type %>
                                            <span class="badge float-right">
                                                <%= user_history[i].TimeCompleted %>
                                            </span>
                                        </div>
                                        <div class="product-description">
                                            <%= user_history[i].status %>
                                        </div>
                                    </li>
                                <% } %>
                            <% } else { %>
                                <li class="item">
                                    <div class="product-title">History is empty!</div>
                                    <div class="product-description">
                                        Completed experiments appear here
                                    </div>
                                </li>
                            <% } %>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
<!--Body-->


