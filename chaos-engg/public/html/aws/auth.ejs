<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>AWS Experiment</title>

  <!-- Theme style for progress bar -->
  <link rel="stylesheet" href="/public/dist/css/progress.css">
  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/public/plugins/fontawesome-free/css/all.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Tempusdominus Bootstrap 4 -->
  <link rel="stylesheet" href="/public/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
  <!-- iCheck -->
  <link rel="stylesheet" href="/public/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
  <!-- JQVMap -->
  <link rel="stylesheet" href="/public/plugins/jqvmap/jqvmap.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/public/dist/css/adminlte.min.css">
  <!-- overlayScrollbars -->
  <link rel="stylesheet" href="/public/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
  <!-- Daterange picker -->
  <link rel="stylesheet" href="/public/plugins/daterangepicker/daterangepicker.css">
  <!-- summernote -->
  <link rel="stylesheet" href="/public/plugins/summernote/summernote-bs4.min.css">
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <!-- Preloader -->
  <div class="preloader flex-column justify-content-center align-items-center">
    <img class="animation__shake" src="/public/dist/img/WiproLogo.png" alt="WiproLogo" height="60" width="60">
  </div>

  <!-- Top Horizontal Navbar -->
  <nav class="main-header navbar navbar-expand navbar-white navbar-light">
    <!-- Left navbar links -->
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
      </li>
      <li class="nav-item d-none d-sm-inline-block">
        <a href="/" class="nav-link">Home</a>
      </li>
    </ul>

    <!-- Right side of top navbar links -->
    <ul class="navbar-nav ml-auto">
      <!-- Navbar Search -->
      <li class="nav-item">
        <a class="nav-link" data-widget="navbar-search" href="#" role="button">
          <i class="fas fa-search"></i>
        </a>
        <div class="navbar-search-block">
          <form class="form-inline">
            <div class="input-group input-group-sm">
              <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
              <div class="input-group-append">
                <button class="btn btn-navbar" type="submit">
                  <i class="fas fa-search"></i>
                </button>
                <button class="btn btn-navbar" type="button" data-widget="navbar-search">
                  <i class="fas fa-times"></i>
                </button>
              </div>
            </div>
          </form>
        </div>
      </li>
    </ul>
  </nav>
  <!-- /.navbar -->


  <!-- Left Side Main Sidebar Container -->
  <aside class="main-sidebar sidebar-dark-primary elevation-4">
    <!-- Brand Logo -->
    <a href="/" class="brand-link">
      <img src="/public/dist/img/WiproLogo.png" alt="Wipro Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
      <span class="brand-text font-weight-light">Wipro</span>
    </a>

    <!-- Sidebar -->
    <div class="sidebar">

      <!-- Sidebar Menu -->
      <nav class="mt-2">
        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
          <!-- Add icons to the links using the .nav-icon class
               with font-awesome or any other icon font library -->
          <li class="nav-item">
            <a href="/" class="nav-link">
              <i class="nav-icon fas fa-tachometer-alt"></i>
              <p>
                Dashboard
              </p>
            </a>
          </li>

          <li class="nav-item">
            <a class="nav-link">
            <i class="nav-icon fas fa-flask"></i>
              <p>
                Create hypothesis
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <div class="nav-link">
                  <i class="fab fa-aws nav-icon"></i>
                  <a href="/aws">
                  <p>AWS</p>
                  </a>
                </div>
              </li>
              <li class="nav-item">
                <div class="nav-link">
                  <i class="fab fa-google nav-icon"></i>
                  <a href="/">
                  <p>GCP</p>
                  </a>
                </div>
              </li>
            </ul>
          </li>
          <li class="nav-item">
            <a class="nav-link">
            <i class="nav-icon far fa-chart-bar"></i>
              <p>
                System metrics
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <div class="nav-link">
                  <i class="fab fa-aws nav-icon"></i>
                  <a href="/aws/metrics">
                  <p>AWS Cloudwatch</p>
                  </a>
                </div>
              </li>
            </ul>
          </li>
        </ul>
      </nav>
      <!-- /.sidebar-menu -->
    </div>
    <!-- /.sidebar -->
  </aside>


<!-- MAIN CONTENT HERE-->
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">AWS Chaos</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/">Home</a></li>
              <li class="breadcrumb-item active">AWS</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Main row -->
            <div class="stepper-wrapper">
                <div class="stepper-item active">
                  <div class="step-counter">1</div>
                  <div class="step-name">Credentials</div>
                </div>
                <div class="stepper-item">
                  <div class="step-counter">2</div>
                  <div class="step-name">Service</div>
                </div>
                <div class="stepper-item">
                  <div class="step-counter">3</div>
                  <div class="step-name">Activity</div>
                </div>
                <div class="stepper-item">
                  <div class="step-counter">4</div>
                  <div class="step-name">Parameters</div>
                </div>
                <div class="stepper-item">
                    <div class="step-counter">5</div>
                    <div class="step-name">Result</div>
                </div>
              </div>
      </div><!-- /.container-fluid -->
    <div  style="padding-left:20%; padding-right:20%;">
        <div class="card card-primary">
            <form action="" method="POST">
                <div class="card-body">
                    <% if(cred.length > 0) { %>
                        <div class="card card-default">
                            <div class="card-header">
                                <h5>Previous credentials found:</h5>
                            </div>
                            <div class="card-body">
                                <div class="form-group">
                                    <% var labels = ['AWS Access key :', 'Secret Key:', 'Region :'] %>
                                    <table border = 0>
                                        <% for(i in cred) { %>
                                            <% if (cred[i] !== '[default]') { %>
                                                <% var key = cred[i].split(' ')[2] %>
                                                <% if(key !== undefined){ %>
                                                    <% if(key.length > 15){ %>    
                                                        <% key = key.substring(0, key.length - 5).replace(/[a-z\d+-_*&%$#@!^]/gi,"*") + key.substring(key.length - 5, key.length) %>
                                                    <% } %>
                                                <% } %>
                                                <tr><td><%= labels[i - 1] %></td><td>&nbsp&nbsp&nbsp<%= key %></td></tr>
                                            <% } %>
                                        <% } %>
                                    </table>
                                </div>
                                <div class="form-group">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="prev_creds" name="prev_creds" value="true" checked>
                                        <label class="form-check-label"><b>Use these credentials?</b></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <h4><b>Or enter new credentials below:</b></h4>
                    <% } %>
                    <div class="form-group">
                        <% if (validate[0]) {%>
                            <div style="color: red"><i class="fas fa-asterisk"></i>Field is required</div>
                        <% } %>
                        <label for="access_key">Enter AWS access key:</label>
                        <input type='text' name="access_key" class="form-control" id="access_key" placeholder="Enter access key">
                    </div>
                    <div class="form-group">
                        <% if (validate[1]) {%>
                            <div style="color: red"><i class="fas fa-asterisk"></i>Field is required</div>
                        <% } %>
                        <label for="secret_access_key">Enter secret access key:</label>
                        <input type='text' name="secret_access_key" class="form-control" id="secret_access_key" placeholder="Enter secret access key">
                    </div>
                    <div class="form-group">
                        <% if (validate[2]) {%>
                            <div style="color: red"><i class="fas fa-asterisk"></i>Field is required</div>
                        <% } %>
                        <label for="region">Enter region:</label>
                        <input type='text' name="region" class="form-control" id="region" placeholder="Enter region">
                    </div>
                </div>
                <div class="card-footer">
                    <button type="submit" class="btn btn-primary float-right">Next</button>
                </div>
            </form>
            
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
<!--Body-->


<!-- jQuery -->
<script src="/public/plugins/jquery/jquery.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="/public/plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
  $.widget.bridge('uibutton', $.ui.button)
</script>
<!-- Bootstrap 4 -->
<script src="/public/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- JQVMap -->
<script src="/public/plugins/jqvmap/jquery.vmap.min.js"></script>
<script src="/public/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
<!-- jQuery Knob Chart -->
<script src="/public/plugins/jquery-knob/jquery.knob.min.js"></script>
<!-- daterangepicker -->
<script src="/public/plugins/moment/moment.min.js"></script>
<script src="/public/plugins/daterangepicker/daterangepicker.js"></script>
<!-- Tempusdominus Bootstrap 4 -->
<script src="/public/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
<!-- Summernote -->
<script src="/public/plugins/summernote/summernote-bs4.min.js"></script>
<!-- overlayScrollbars -->
<script src="/public/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
<!-- AdminLTE App -->
<script src="/public/dist/js/adminlte.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="/public/dist/js/demo.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="/public/dist/js/pages/dashboard.js"></script>
</body>
</html>
